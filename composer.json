{
    "name": "michaelesmith/throttle",
    "description": "A basic throttling implementation",
    "keywords": ["throttle"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Michael Smith",
            "email": "sitecross@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1",
        "psr/cache": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.4",
        "mockery/mockery": "^1.2",
        "php-mock/php-mock-mockery": "^1.3"
    },
    "suggest": {
        "cache/cache": "Library of all the php-cache adapters",
        "symfony/cache": "Symfony Cache component with PSR-6, PSR-16, and tags"
    },
    "autoload": {
        "psr-4": {"MS\\Throttle\\": "src"}
    },
    "autoload-dev": {
        "psr-4": {"MS\\Throttle\\Tests\\Unit\\": "tests/unit", "MS\\Throttle\\Tests\\Common\\": "tests/common"}
    }
}
